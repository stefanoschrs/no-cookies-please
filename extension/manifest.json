{
  "name": "No Cookies Please",
  "description": "Remove unwanted cookie popups",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {

  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}